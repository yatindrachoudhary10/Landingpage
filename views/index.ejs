  <section class="our-projects py-5">
  <div class="container">
    <div class="text-center mb-4">
      <h3 class="section-title">Our Projects</h3>
      <p class="section-subtitle">We know what buyers are looking for and suggest projects that will bring<br>clients top dollar for the sale of their homes.</p>
    </div>

    <div class="row justify-content-center">
      <% projects.forEach(project => { %>
        <div class="col-lg-2 col-md-4 col-sm-6 mb-4">
          <div class="card project-card text-center">
            <img src="<%= project.image %>" class="card-img-top" alt="Project image">
            <div class="card-body px-2">
              <h6 class="card-title text-primary"><%= project.name %></h6>
              <p class="card-subtitle">Project Name · Location</p>
              <a href="#" class="btn btn-warning btn-sm mt-2 text-white fw-bold">READ MORE</a>
            </div>
          </div>
        </div>
      <% }) %>
    </div>
  </div>
</section>


<!-- happy client -->
 <section class="happy-clients py-5">
  <div class="container">
    <div class="text-center mb-5">
      <h3 class="section-title">Happy Clients</h3>
    </div>

    <div class="row justify-content-center">
      <% clients.forEach(client => { %>
        <div class="col-lg-3 col-md-4 col-sm-6 mb-4">
          <div class="card h-100 text-center p-3 shadow-sm border-0 rounded-4">
            <img src="<%= client.image %>" 
                 class="rounded-circle mx-auto mb-3" 
                 style="width: 60px; height: 60px; object-fit: cover;" 
                 alt="<%= client.name %>">
            
            <p class="text-muted small"><%= client.description %></p>
            <h6 class="fw-bold text-primary mb-0"><%= client.name %></h6>
            <small class="text-muted"><%= client.designation %></small>
          </div>
        </div>
      <% }) %>
    </div>
  </div>
</section>

<!-- learn more -->

<div class="learn-more">
  <img src="/images/Rectangle.svg" alt="learn more image" class="background-img">
  <div class="content-overlay">
    <h3>
      Learn more about our listing process, as well as our <br>
      additional staging and design work.
    </h3>
    <button>LEARN MORE</button>
  </div>
</div>

<!-- footer1 -->
 <div>
  <footer class="footer">
  <div class="footer-content">
    <ul class="footer-nav">
      <li><a href="#">Home</a></li>
      <li><a href="#">Services</a></li>
